{
  "name": "bundled-eslint-config",
  "version": "0.3.5",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc",
    "build": "esbuild --bundle src/index.ts --outfile=dist/index.js --format=esm --platform=node --sourcemap=linked --packages=external"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint-types/typescript-eslint": "^7.5.0",
    "@eslint-types/unicorn": "^52.0.0",
    "@types/node": "^22.5.1",
    "esbuild": "^0.23.1",
    "some-tsconfig-preset": "^1.5.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@brettz9/eslint-plugin": "^3.0.0",
    "@eslint/compat": "^1.1.1",
    "@stylistic/eslint-plugin-js": "^2.7.1",
    "@stylistic/eslint-plugin-plus": "^2.7.1",
    "@stylistic/eslint-plugin-ts": "^2.7.1",
    "@vitest/eslint-plugin": "^1.1.0",
    "astro-eslint-parser": "^1.0.2",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-case-police": "^0.7.0",
    "eslint-plugin-import-x": "^4.1.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "typescript-eslint": "8.2.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependencies": {
    "eslint": "^9.9.1"
  },
  "main": "dist/index.js",
  "description": "A preset for ESLint",
  "keywords": [
    "eslint",
    "eslint preset",
    "preset",
    "typescript eslint preset",
    "typescript-eslint",
    "vue eslint preset",
    "astro eslint preset",
    "vitest eslint preset",
    "react eslint preset"
  ],
  "author": "DaniFoldi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaniFoldi/lint-my-life/issues"
  },
  "homepage": "https://github.com/DaniFoldi/lint-my-life#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniFoldi/lint-my-life.git"
  }
}
