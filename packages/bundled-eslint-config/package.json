{
  "name": "bundled-eslint-config",
  "version": "0.2.7",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc",
    "build": "esbuild --bundle src/index.ts --outfile=dist/index.js --format=esm --platform=node --sourcemap=linked --packages=external"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint-types/typescript-eslint": "^6.21.0",
    "@eslint-types/unicorn": "^50.0.1",
    "@types/node": "^20.11.19",
    "esbuild": "^0.20.0",
    "some-tsconfig-preset": "^1.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "dependencies": {
    "@brettz9/eslint-plugin": "^1.0.4",
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@stylistic/eslint-plugin-plus": "^1.6.2",
    "@stylistic/eslint-plugin-ts": "^1.6.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "astro-eslint-parser": "^0.16.3",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^0.1.3",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-case-police": "^0.6.1",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-playwright": "^0.22.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-tailwindcss": "^3.14.2",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-vitest": "^0.3.22",
    "eslint-plugin-vue": "^9.21.1",
    "globals": "^13.24.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "main": "dist/index.js",
  "description": "A preset for ESLint",
  "keywords": [
    "eslint",
    "eslint preset",
    "preset",
    "typescript eslint preset",
    "typescript-eslint",
    "vue eslint preset",
    "astro eslint preset",
    "vitest eslint preset",
    "react eslint preset"
  ],
  "author": "DaniFoldi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaniFoldi/lint-my-life/issues"
  },
  "homepage": "https://github.com/DaniFoldi/lint-my-life#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniFoldi/lint-my-life.git"
  }
}
