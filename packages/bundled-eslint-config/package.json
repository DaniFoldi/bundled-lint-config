{
  "name": "bundled-eslint-config",
  "version": "0.7.8",
  "description": "A preset for ESLint",
  "keywords": [
    "eslint",
    "eslint preset",
    "preset",
    "typescript eslint preset",
    "typescript-eslint",
    "vue eslint preset",
    "astro eslint preset",
    "vitest eslint preset",
    "react eslint preset"
  ],
  "homepage": "https://github.com/DaniFoldi/bundled-lint-config#readme",
  "bugs": {
    "url": "https://github.com/DaniFoldi/bundled-lint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniFoldi/bundled-lint-config.git"
  },
  "license": "MIT",
  "author": "DaniFoldi",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild --format=esm --bundle src/index.ts --outdir=dist --target=node20 --platform=node --packages=external --sourcemap=external",
    "prelint": "pnpm build",
    "lint": "eslint --cache .",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@brettz9/eslint-plugin": "^3.0.0",
    "@eslint/json": "^0.14.0",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@vitest/eslint-plugin": "^1.6.6",
    "astro-eslint-parser": "^1.2.2",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-case-police": "^2.1.1",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-import-zod": "^1.2.1",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-json-schema-validator": "^6.0.3",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-package-json": "^0.88.2",
    "eslint-plugin-playwright": "^2.5.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.7.0",
    "find-up": "^8.0.0",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^2.4.2",
    "typescript-eslint": "^8.54.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@types/node": "24.10.9",
    "esbuild": "0.27.2",
    "jiti": "2.6.1",
    "some-tsconfig-preset": "3.0.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "eslint": "^9.39.2"
  }
}
