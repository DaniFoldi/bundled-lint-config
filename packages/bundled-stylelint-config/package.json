{
  "name": "bundled-stylelint-config",
  "version": "0.1.8",
  "description": "A preset for stylelint",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc",
    "build": "esbuild --bundle src/index.ts --outfile=dist/index.js --format=esm --platform=node --sourcemap=linked --packages=external"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "stylelint preset",
    "stylelint config",
    "stylelint",
    "lint",
    "stylelint scss preset"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "esbuild": "^0.24.2",
    "some-tsconfig-preset": "^1.6.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@stylistic/stylelint-plugin": "^3.1.1",
    "find-up": "^7.0.0",
    "parse-gitignore": "^2.0.0",
    "postcss": "^8.4.49",
    "postcss-scss": "^4.0.9",
    "stylelint": "^16.12.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-high-performance-animation": "^1.10.0",
    "stylelint-scss": "^6.10.0"
  }
}
