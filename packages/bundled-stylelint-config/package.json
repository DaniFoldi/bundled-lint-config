{
  "name": "bundled-stylelint-config",
  "version": "0.1.10",
  "description": "A preset for stylelint",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc",
    "build": "esbuild --bundle src/index.ts --outfile=dist/index.js --format=esm --platform=node --sourcemap=linked --packages=external"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "stylelint preset",
    "stylelint config",
    "stylelint",
    "lint",
    "stylelint scss preset"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "25.2.2",
    "esbuild": "0.27.3",
    "some-tsconfig-preset": "3.0.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@stylistic/stylelint-plugin": "^5.0.1",
    "find-up": "^8.0.0",
    "parse-gitignore": "^2.0.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "stylelint": "^17.1.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-high-performance-animation": "^2.0.0",
    "stylelint-scss": "^7.0.0"
  }
}
